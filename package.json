{
  "name": "spare-parts",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "config": {
    "sourcemap": "cheap-module-eval-source-map",
    "karma_browsers": "Firefox",
    "karma_sourcemap": true
  },
  "scripts": {
    "preclean": "echo 'clean: start'",
    "clean": "rimraf build",
    "postclean": "echo 'clean: done'",
    "prelint": "echo 'lint: start'",
