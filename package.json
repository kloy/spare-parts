    "test:watch": "cross-env NODE_ENV=test karma start tools/config/karma.conf.js --auto-watch",
    "dist": "cross-env NODE_ENV=production webpack --config tools/config/webpack.config.js --progress --display-error-details",
    "npm-check-updates": "ncu"
  },
