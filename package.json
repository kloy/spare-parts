    "eslint-config-airbnb": "4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "~0.2.0",
    "karma-firefox-launcher": "^0.1.7",
