{
    "extends": "airbnb",
    "rules": {
        "prefer-const": 0,
        "indent": [1, 4, { "SwitchCase": 1 }],
        "func-names": 0,
        "comma-dangle": 0,
        "id-length": 0,
        "no-unused-vars": 1,
        "vars-on-top": 0,
        "no-var": 1,
        "no-shadow": 1,
        "one-var": 1,
        "key-spacing": 1,
        "padded-blocks": 1,
        "comma-spacing": 1,
        "quotes": [1, "single"],
        "new-cap": 1,
        "no-new": 0,
        "no-floating-decimal": 1,
        "spaced-comment": 1,
        "space-infix-ops": 1,
        "eol-last": 1,
        "no-trailing-spaces": 1,
        "no-irregular-whitespace": 1,
        "dot-notation": 1,
        "no-param-reassign": 1,
        "space-before-blocks": 1,
        "no-multiple-empty-lines": 1,
        "no-unused-expressions": 1,
        "eqeqeq": 1,
        "no-cond-assign": 1,
        "consistent-return": 1,
        "object-shorthand": 0,
        "prefer-arrow-callback": 0,
        "strict": 0,
        "no-var": 0,
        "prefer-template": 0,
        "max-len": [1, 120, 4]
    },
    "globals": {
      "sinon": true,
      "QUnit": true
    }
}
